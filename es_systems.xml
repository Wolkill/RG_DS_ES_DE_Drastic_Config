<?xml version="1.0"?>
<!-- This is the ES-DE game systems configuration file for Android -->
<systemList>
    <system>
        <name>nds</name>
        <fullname>Nintendo DS</fullname>
        <path>%ROMPATH%/nds</path>
        <extension>.app .APP .bin .BIN .nds .NDS .7z .7Z .zip .ZIP</extension>
		<command label="DraStic (Anbernic RG DS)">%EMULATOR_DRASTIC% %ACTIVITY_CLEAR_TASK% %ACTIVITY_CLEAR_TOP% %DATA%=%ROM%</command>
        <platform>nds</platform>
        <theme>nds</theme>
    </system>
</systemList>
